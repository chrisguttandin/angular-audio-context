{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "defaultProject": "angular-audio-context",
    "newProjectRoot": "projects",
    "projects": {
        "angular-audio-context": {
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "configurations": {},
                    "options": {
                        "assets": [],
                        "buildOptimizer": false,
                        "extractLicenses": false,
                        "index": "src/index.html",
                        "main": "src/module.ts",
                        "namedChunks": true,
                        "optimization": false,
                        "outputPath": "build",
                        "polyfills": ["zone.js"],
                        "scripts": [],
                        "sourceMap": true,
                        "styles": [],
                        "tsConfig": "src/tsconfig.json",
                        "vendorChunk": true
                    }
                },
                "lint": {
                    "builder": "@angular-eslint/builder:lint",
                    "configurations": {
                        "test": {
                            "eslintConfig": "config/eslint/test.json",
                            "lintFilePatterns": ["test/**/*.html", "test/**/*.ts"]
                        }
                    },
                    "options": {
                        "eslintConfig": "config/eslint/src.json",
                        "lintFilePatterns": ["src/**/*.html", "src/**/*.ts"]
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "assets": [],
                        "include": ["../test/unit/**/*.ts"],
                        "karmaConfig": "config/karma/config.js",
                        "polyfills": ["zone.js", "zone.js/testing"],
                        "scripts": [],
                        "styles": [],
                        "tsConfig": "test/unit/tsconfig.json"
                    }
                }
            },
            "projectType": "application",
            "root": "",
            "schematics": {},
            "sourceRoot": "src"
        }
    },
    "version": 1
}
